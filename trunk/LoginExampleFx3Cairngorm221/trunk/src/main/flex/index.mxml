<?xml version="1.0" encoding="utf-8"?>
<mx:Application 
	xmlns:mx="http://www.adobe.com/2006/mxml" 
	xmlns:service="com.masuland.loginexample.service.*"
	xmlns:action="com.masuland.loginexample.action.*"
	xmlns:view="com.masuland.loginexample.view.*"
	width="100%" height="100%"
	layout="absolute" 
	backgroundAlpha="0"
	backgroundColor="0xffffff"
	backgroundGradientAlphas="[1, 1]"
	backgroundGradientColors="[#ffffff, #ffffff]"
	pageTitle="Cairngorm 2.2.2 - Login Example (Fx3)"
	applicationComplete="new InitialiseClientEvent().dispatch()"
	viewSourceURL="srcview/index.html"
	>

	<mx:Metadata>
		[ResourceBundle("resources")]
	</mx:Metadata>
	
	<mx:Script>
		<![CDATA[
			import com.masuland.loginexample.action.event.InitialiseClientEvent;
		]]>
	</mx:Script>
	
	<service:AppServices
		id="services"
		/>
	
	<action:AppController
		id="controller"
		/>
	
	<view:AppView
		id="view"
		/>
	
</mx:Application>
